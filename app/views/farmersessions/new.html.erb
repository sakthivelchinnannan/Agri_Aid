<% provide :title, "Sign in" %>
<h1>Sign in</h1>
<%= form_for(:farmersession, url: farmersessions_path) do |f| %>
  <div class="field">
    <%= f.label :phone %><br />
    <%= f.text_field :phone %>
  </div>
  
  <div class="actions">
    <%= f.submit "Sign in" %>
  </div>
<% end %>
<p>New user? <%= link_to "Sign up now!", farmersignup_path %></p>
