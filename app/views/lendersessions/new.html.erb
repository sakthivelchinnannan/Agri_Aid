<% provide :title, "Sign in" %>
<h1>Sign in</h1><br />
<%= form_for(:lendersession, url: lendersessions_path) do |f| %>
  <div class="field">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
<!--
  <div class="field">
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
-->
  <div class="actions">
    <%= f.submit "Sign in" %>
  </div>
<% end %>
